    "@types/node": "24.0.15",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "typescript": "5.8.3",
    "vitest": "3.2.4",
    "wrangler": "4.16.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md,yaml,yml}": [
      "biome check --write --files-ignore-unknown=true"
    ]
  },
  "dependencies": {
    "@cloudflare/ai-utils": "1.0.1",
    "@nekofar/warpcast": "2.0.0-alpha.34",
