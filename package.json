{
  "private": true,
  "name": "lilnouns-agent",
  "version": "1.0.0-alpha.0",
  "description": "",
  "author": "",
  "license": "Apache-2.0",
  "scripts": {
    "build": "wrangler deploy --dry-run",
    "dev": "wrangler dev --test-scheduled",
    "deploy": "wrangler deploy",
    "start": "wrangler dev --test-scheduled",
    "test": "vitest --config vitest.config.ts",
    "test:run": "vitest run --config vitest.config.ts",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "prepare": "husky",
    "cf-typegen": "wrangler types --include-runtime=false --strict-vars=false"
  },
  "packageManager": "pnpm@10.12.1",
  "keywords": [],
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@cloudflare/vitest-pool-workers": "^0.8.53",
    "@types/node": "^24.0.15",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "wrangler": "4.16.1"
    "@cloudflare/workers-types": "4.20250719.0",
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md,yaml,yml}": [
      "biome check --write --files-ignore-unknown=true"
    ]
  },
  "dependencies": {
    "@nekofar/warpcast": "2.0.0-alpha.26"
  }
}
